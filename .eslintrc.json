{
  "env": {
    "es6": true
  },
  "extends": [
    "prettier-standard"
  ],
  "rules": {
    "eqeqeq": 0
  },
  "globals": {
    "Bangle": true,
    "BTN1": true,
    "BTN2": true,
    "BTN3": true,
    "BTN4": true,
    "BTN5": true,
    "clearWatch": true,
    "E": true,
    "g": true,
    "Modules": true,
    "reset": true,
    "setWatch": true
  }
}
